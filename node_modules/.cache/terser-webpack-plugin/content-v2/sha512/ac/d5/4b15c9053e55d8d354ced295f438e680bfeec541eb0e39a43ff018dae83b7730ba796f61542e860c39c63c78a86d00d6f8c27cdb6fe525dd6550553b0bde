ioList,\n  resetAnswer,\n  endGame,\n  resetExtraScore,\n} from \"../../redux/birds/birds.action\";\n\nconst CustomButton = () => {\n  const dispatch = useDispatch();\n  const level = useSelector(selectLevel);\n  const answered = useSelector(selectAnswered);\n\n  const handleGoNextLevel = () => {\n    if (answered) {\n      if (level === 5) {\n        dispatch(endGame());\n      } else {\n        dispatch(increaseLevel());\n        dispatch(chooseCurrentList(null));\n        dispatch(cleanBirdInfo());\n        dispatch(changeNameList(level));\n        dispatch(changeAudioList(level));\n        dispatch(resetAnswer());\n        dispatch(goNextLevelStart());\n        dispatch(resetExtraScore());\n      }\n    }\n  };\n\n  return (\n    <div>\n      <button\n        className={answered ? \"custom-button\" : \"blockedBtn\"}\n        onClick={() => {\n          handleGoNextLevel();\n        }}\n      >\n        Следующий уровень\n      </button>\n    </div>\n  );\n};\n\nexport default CustomButton;\n","import React from \"react\";\n\nimport \"./endgame.styles.scss\";\nimport { selectScore } from \"../../redux/birds/birds.selector\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { reestGame, fetchImageStart } from \"../../redux/birds/birds.action\";\n\nconst EndGame = () => {\n  const dispatch = useDispatch();\n\n  const handleEndGame = () => {\n    dispatch(reestGame());\n    dispatch(fetchImageStart());\n  };\n\n  const score = useSelector(selectScore);\n  return (\n    <div className=\"endgame-container\">\n      <div className=\"endgame-title\">Поздравляем</div>\n      <div className=\"endgame-subtitle\">\n        Игра окончена. Общий результат {score} из 24 возможных баллов.\n      </div>\n      {score == 24 ? (\n        <div className=\"endgame-subtitle\">Поздравляем, Вы прошли игру.</div>\n      ) : (\n        <button className=\"endgame-btn\" onClick={() => handleEndGame()}>\n          Попробовать еще раз!\n        </button>\n      )}\n    </d